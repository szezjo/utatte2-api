{
  "name": "utatte2-api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/app.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/app.js\"",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format:check": "prettier --config .prettierrc --check src/.",
    "format": "prettier --write src/.",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "npm run format:check",
      "npm run lint"
    ]
  },
  "dependencies": {
    "@types/cors": "^2.8.13",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.191",
    "@types/wanakana": "^4.0.3",
    "body-parser": "^1.20.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "fs-extra": "^11.1.0",
    "http-errors": "~1.6.3",
    "kana-romaji": "^0.1.10",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11",
    "sqlite": "^4.1.2",
    "sqlite3": "^5.1.6",
    "wanakana": "^5.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "axios": "^1.1.3",
    "concurrently": "^7.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "typescript": "^4.9.3",
    "xml2js": "^0.4.23"
  }
}
